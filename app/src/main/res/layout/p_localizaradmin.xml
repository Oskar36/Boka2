<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:name="com.google.android.gms.maps.MapFragment"
    tools:context=".a_localizacion"
    tools:ignore="ExtraText">


    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView4"
        android:layout_width="412dp"
        android:layout_height="523dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/TxtComensales"
        style="@style/Contenedores"
        android:layout_width="265dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toTopOf="@+id/mapView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.322"
        android:textAlignment="gravity"
        android:gravity="center"/>

    <EditText
        android:foregroundGravity="left"
        android:textAlignment="gravity"
        android:id="@+id/txtFechaADM"
        android:inputType="time"
        style="@style/Contenedores"
        android:layout_width="265dp"
        android:layout_height="50dp"
        android:gravity="center"
        app:layout_constraintBottom_toTopOf="@+id/mapView4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TxtComensales"
        app:layout_constraintVertical_bias="0.641"
        android:clickable="false"
        android:editable="false"
        android:ems="10"
        android:focusable="false"
        android:hint="@string/fecha"/>

</androidx.constraintlayout.widget.ConstraintLayout >