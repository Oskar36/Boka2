<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".a_reservas">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    tools:layout_editor_absoluteX="-9dp"
                    tools:layout_editor_absoluteY="-3dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:id="@+id/linearLayout2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/calletxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            android:layout_marginEnd="436dp"
                            android:text="Calle"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/spinermunicipio" />

                        <EditText
                            android:id="@+id/txtHora"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="19dp"
                            android:layout_marginEnd="261dp"
                            android:ems="10"
                            android:hint="Hora: HH:mm"
                            android:inputType="time"
                            android:minHeight="48dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/txtfecha" />

                        <EditText
                            android:id="@+id/txtfecha"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="31dp"
                            android:layout_marginEnd="261dp"
                            android:ems="10"
                            android:hint="Fecha: DD/MM/AAAA"
                            android:inputType="date"
                            android:minHeight="48dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/munitxt"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="408dp"
                            android:layout_marginBottom="8dp"
                            android:text="Municipio"
                            app:layout_constraintBottom_toTopOf="@+id/spinermunicipio"
                            app:layout_constraintEnd_toEndOf="parent" />

                        <EditText
                            android:id="@+id/txtcomensales"
                            android:layout_width="48dp"
                            android:layout_height="48dp"
                            android:layout_marginTop="7dp"
                            android:ems="10"
                            android:inputType="number"
                            app:layout_constraintEnd_toStartOf="@+id/btnup"
                            app:layout_constraintTop_toBottomOf="@+id/txtHora"
                            tools:ignore="SpeakableTextPresentCheck" />

                        <ImageView
                            android:id="@+id/imageView2"
                            android:layout_width="49dp"
                            android:layout_height="38dp"
                            android:layout_marginTop="7dp"
                            android:layout_marginEnd="46dp"
                            app:layout_constraintEnd_toStartOf="@+id/txtcomensales"
                            app:layout_constraintTop_toBottomOf="@+id/txtHora"
                            app:srcCompat="@drawable/iconousuarioverde" />

                        <ImageButton
                            android:id="@+id/btnup"
                            android:layout_width="33dp"
                            android:layout_height="17dp"
                            android:layout_marginTop="7dp"
                            android:layout_marginEnd="273dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/txtHora"
                            app:srcCompat="@android:drawable/arrow_up_float"
                            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                        <ImageButton
                            android:id="@+id/btndown"
                            android:layout_width="33dp"
                            android:layout_height="21dp"
                            android:layout_marginEnd="273dp"
                            android:layout_marginBottom="34dp"
                            app:layout_constraintBottom_toTopOf="@+id/spinermunicipio"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:srcCompat="@android:drawable/arrow_down_float"
                            tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

                        <Spinner
                            android:id="@+id/spinermunicipio"
                            android:layout_width="210dp"
                            android:layout_height="48dp"

                            android:layout_marginTop="18dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/txtcomensales"
                            tools:ignore="SpeakableTextPresentCheck" />

                        <Spinner
                            android:id="@+id/spinercalle"
                            android:layout_width="208dp"
                            android:layout_height="48dp"
                            android:layout_marginTop="40dp"
                            android:layout_marginEnd="260dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/spinermunicipio"
                            tools:ignore="SpeakableTextPresentCheck" />

                        <ImageView
                            android:id="@+id/imageView5"
                            android:layout_width="130dp"
                            android:layout_height="119dp"
                            android:layout_marginTop="6dp"
                            android:layout_marginEnd="155dp"
                            app:layout_constraintEnd_toStartOf="@+id/btnreserva"
                            app:layout_constraintTop_toBottomOf="@+id/spinercalle"
                            app:srcCompat="@drawable/mapa" />

                        <Button
                            android:id="@+id/btnreserva"
                            style="@style/botones"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="16dp"
                            android:layout_marginBottom="24dp"
                            android:text="RESERVAR"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent" />
                    </androidx.constraintlayout.widget.ConstraintLayout>
                </ScrollView>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout><![CDATA[



    app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.377"
        tools:ignore="SpeakableTextPresentCheck" />













    ]]>
</androidx.constraintlayout.widget.ConstraintLayout>